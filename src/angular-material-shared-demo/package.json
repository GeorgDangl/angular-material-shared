{
  "name": "angular-material-shared-demo",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npm run copy:icons && npm run copy:plugins && npm run copy:skins && npm run copy:themes && npm run copy:langs",
    "copy:icons": "xcopy /I /E /Y node_modules\\tinymce\\icons src\\assets\\tinymce-assets\\icons",
    "copy:plugins": "xcopy /I /E /Y node_modules\\tinymce\\plugins src\\assets\\tinymce-assets\\plugins",
    "copy:skins": "xcopy /I /E /Y node_modules\\tinymce\\skins src\\assets\\tinymce-assets\\skins",
    "copy:themes": "xcopy /I /E /Y node_modules\\tinymce\\themes src\\assets\\tinymce-assets\\themes",
    "copy:langs": "xcopy /I /E /Y node_modules\\tinymce-i18n\\langs5 src\\assets\\tinymce-assets\\langs",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:library": "ng build angular-material-shared --configuration production && xcopy projects\\angular-material-shared\\src\\styles dist\\angular-material-shared\\styles\\ /E",
    "test:ci": "ng test angular-material-shared --karma-config projects/angular-material-shared/karma-ci.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^13.2.3",
    "@angular/animations": "^13.2.2",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "^13.2.2",
    "@angular/compiler": "^13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/forms": "^13.2.2",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "^13.2.2",
    "@angular/router": "^13.2.2",
    "core-js": "^3.21.0",
    "rxjs": "^7.3.0",
    "tinymce": "^5.10.3",
    "tinymce-i18n": "^22.2.11",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "^13.2.2",
    "@angular/language-service": "^13.2.2",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^17.0.18",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^13.2.1",
    "protractor": "~7.0.0",
    "puppeteer": "^10.2.0",
    "ts-node": "~10.2.1",
    "tslint": "~6.1.0",
    "typescript": "~4.5.5"
  }
}
