{
  "name": "angular-material-shared-demo",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npm run copy:icons && npm run copy:plugins && npm run copy:skins && npm run copy:themes && npm run copy:langs && npm run copy:models",
    "copy:icons": "xcopy /I /E /Y node_modules\\tinymce\\icons src\\assets\\tinymce-assets\\icons",
    "copy:plugins": "xcopy /I /E /Y node_modules\\tinymce\\plugins src\\assets\\tinymce-assets\\plugins",
    "copy:skins": "xcopy /I /E /Y node_modules\\tinymce\\skins src\\assets\\tinymce-assets\\skins",
    "copy:themes": "xcopy /I /E /Y node_modules\\tinymce\\themes src\\assets\\tinymce-assets\\themes",
    "copy:langs": "xcopy /I /E /Y node_modules\\tinymce-i18n\\langs5 src\\assets\\tinymce-assets\\langs",
    "copy:models": "xcopy /I /E /Y node_modules\\tinymce\\models src\\assets\\tinymce-assets\\models",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:library": "ng-packagr -p  projects/angular-material-shared/ng-package.json && xcopy projects\\angular-material-shared\\src\\styles dist\\angular-material-shared\\styles\\ /E",
    "test:ci": "ng test angular-material-shared --karma-config projects/angular-material-shared/karma-ci.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^18.0.1",
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@tinymce/tinymce-angular": "^8.0.0",
    "core-js": "^3.21.0",
    "rxjs": "^7.5.5",
    "tinymce": "^6.8.3",
    "tinymce-i18n": "^24.3.11",
    "tslib": "^2.3.1",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.1",
    "@angular/cli": "^18.0.1",
    "@angular/compiler-cli": "^18.0.0",
    "@angular/language-service": "^18.0.0",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^20.2.1",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^18.0.0",
    "protractor": "~7.0.0",
    "puppeteer": "^22.13.0",
    "ts-node": "~10.2.1",
    "tslint": "~6.1.0",
    "typescript": "~5.4.5"
  }
}
